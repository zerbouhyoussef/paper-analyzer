{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(api_requests_total[1m]))",
          "legendFormat": "total",
          "refId": "A"
        },
        {
          "expr": "sum(rate(api_requests_total{status=~\"5..\"}[1m]))",
          "legendFormat": "5xx errors",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Request Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(api_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(api_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(api_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Papers Loaded",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "api_papers_loaded_total",
          "legendFormat": "papers",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Search Queries (total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "api_search_queries_total",
          "legendFormat": "queries",
          "refId": "A"
        }
      ]
    },
    {
      "title": "API Uptime",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } }, "type": "value" }
          ]
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "up{job=\"api\"}",
          "legendFormat": "api",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Search Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(api_search_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(api_search_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Requests by Endpoint",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "bars", "fillOpacity": 50, "lineWidth": 1, "stacking": { "mode": "normal" } }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(api_requests_total[1m])) by (endpoint)",
          "legendFormat": "{{ endpoint }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Requests by Status Code",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(api_requests_total[1h])) by (status)",
          "legendFormat": "{{ status }}",
          "refId": "A"
        }
      ]
    }
  ],
